class _ViewUtils {
  showSideNav() {
    $('.hamburger-button').sideNav('show');
  }

  get isMobile() {
    return $(window).width() <= 992;
  }

  goBack() {
    if (history.length == 1) {
      location.replace('/#!/record');
    }
    else {
      history.back();
    }
  }
}

const ViewUtils = new _ViewUtils();
export default ViewUtils;
